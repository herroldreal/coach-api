{
  "name": "coach-api",
  "version": "0.0.1",
  "description": "Coach API - AI - GPT-3",
  "author": "Herrold Real <realherrold@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/herroldreal/coach-api.git"
  },
  "bugs": {
    "url": "https://github.com/herroldreal/coach-api/issues"
  },
  "homepage": "https://github.com/herroldreal/coach-api#readme",
  "scripts": {
    "prepare": "husky install",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest --verbose ./tests --maxWorkers=1 --forceExit --detectOpenHandles --silent",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config tests/config/jest-e2e.json"
  },
  "dependencies": {
    "@hapi/joi": "17.1.1",
    "@nestjs/common": "9.3.5",
    "@nestjs/core": "9.3.5",
    "@nestjs/passport": "9.0.1",
    "@nestjs/platform-express": "9.3.5",
    "@nestjs/swagger": "6.2.1",
    "@prisma/client": "4.10.0",
    "@sentry/integrations": "7.36.0",
    "@sentry/node": "7.36.0",
    "@sentry/tracing": "7.36.0",
    "axios": "1.3.2",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "helmet": "6.0.1",
    "jsonwebtoken": "9.0.0",
    "lint-staged": "13.1.1",
    "module-alias": "2.2.2",
    "mongodb": "5.0.1",
    "multer": "1.4.5-lts.1",
    "openai": "3.1.0",
    "passport": "0.6.0",
    "passport-facebook-token": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.0",
    "swagger-ui-express": "4.6.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@faker-js/faker": "7.6.0",
    "@nestjs/cli": "9.2.0",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.3.5",
    "@shelf/jest-mongodb": "4.1.6",
    "@types/aws-lambda": "8.10.110",
    "@types/axios": "0.14.0",
    "@types/express": "4.17.17",
    "@types/hapi__joi": "17.1.9",
    "@types/jest": "29.4.0",
    "@types/node": "18.13.0",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "eslint": "8.33.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "jest": "29.4.2",
    "prettier": "2.8.3",
    "prisma": "4.10.0",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.5"
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "lint-staged": {
    "*.ts": [
      "prettier --ignore-path .eslintignore --write \"**/*.ts\"",
      "eslint --ignore-path .eslintignore  ."
    ]
  },
  "_moduleAliases": {
    "@App": "./dist",
    "@Utils": "dist/utils",
    "@Services": "dist/services",
    "@Logger": "dist/utils/logger",
    "@Db": "dist/db"
  },
  "prima": {
    "schema": "prisma/schema.prisma"
  }
}
